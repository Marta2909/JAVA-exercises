import java.util.Scanner;

public class SimpleOutput {

	public static void main(String[] args) {
		String liczba;
		Scanner odczyt = new Scanner(System.in);
		liczba = odczyt.nextLine();
		while (liczba != "42") {
			odczyt = new Scanner(System.in);
			liczba = odczyt.nextLine();
		 System.out.println(liczba);
		}
	}

}
